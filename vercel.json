{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/$1" }
    ],
    "crons": [{
        "path": "/api/cron",
        "schedule": "0 10 * * *"
      }]
  }