{
  "version": 3,
  "routes": [
    {
      "src": "^/_astro/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^\\/_image$",
      "dest": "_render"
    },
    {
      "src": "/.*",
      "dest": "/404.html",
      "status": 404
    }
  ],
  "framework": {
    "version": "4.11.5"
  },
  "crons": []
}
