---
const res = await fetch(import.meta.env.PUBLIC_NOETICS_CMS_API_URL + "/posts");
const posts = await res.json();
---

<h1>Blog</h1>
{
    posts.map((post: { title: unknown; content: unknown }) => (
        <article>
            <h2>{post.title}</h2>
            <div set:html={post.content} />
        </article>
    ))
}
